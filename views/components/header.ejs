<% const header = components.header.component_options; %>

<div id="Header">
  <div class="Loadbar" id="Loadbar">
    <% if (data.posts === null || data.posts.length < 1) { %>
    <div class="passed"></div>
    <% } %>
  </div>
  <div class="Logo">
    <img src="<%= assets('/images/' + header.logo.name + header.logo.extension) %>" />
  </div>
  <h1 class="Title"><%= header.title %></h1>
</div>
